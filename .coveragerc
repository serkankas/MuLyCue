[run]
source = src
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */site-packages/*
    */launcher.py
    */build.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov

